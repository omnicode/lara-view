{
	"name": "omnicode/lara-view",
	"description": "Blade files for CRUD methods",
	"keywords": ["laravel", "crud", "blade"],
	"license": "MIT",
	"authors": [
		{
			"name": "Omnicode"
		}
	],
	"require": {
		"laravel/framework": "5.5.*",
		"omnicode/lara-link": "~2.0",
		"laracasts/flash": "~2.0 || ~3.0"
	},
	"require-dev": {
		"mockery/mockery": "~1.0",
		"phpunit/phpunit": "~6.0"
	},
	"suggest": {
		"omnicode/lara-view-composer": "Useful view composers to populate dropdown lists"
	},
	"autoload": {
		"psr-4": {
			"LaraView\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
}
